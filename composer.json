{
  "name": "notamedia/yii2-sentry",
  "description": "Yii2 logger for Sentry",
  "keywords": [
    "sentry",
    "yii2"
  ],
  "type": "yii2-extension",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "~2.1",
    "sentry/sentry": "^1.1"
  },
  "require-dev": {
    "codeception/codeception": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "notamedia\\sentry\\": "src"
    }
  }
}
