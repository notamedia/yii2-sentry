{
  "name": "notamedia/yii2-sentry",
  "description": "Yii2 logger for Sentry",
  "keywords": [
    "sentry",
    "yii2"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "yiisoft/yii2": "^2.0",
    "sentry/sdk": "^2.0"
  },
  "require-dev": {
    "codeception/codeception": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "notamedia\\sentry\\": "src"
    }
  },
  "extra": {
    "asset-vcs-driver-options": {
      "github-no-api": true
    },
    "asset-pattern-skip-version": "(-build|-patch)"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
